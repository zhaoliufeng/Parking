/* pages/license_plate/licenese_plate.wxss */

#plate {
  border-radius: 20rpx;
  margin: 10rpx;
  display: flex;
  width: 97%;
  height: 200rpx;
  background: #0047b5;
}

#plate_border {
  white-space:nowrap;
  letter-spacing: 5rpx;
  color: white;
  font-size: 80rpx;
  line-height: 172rpx;
  text-align: center;
  margin-left: 2.2%;
  border-radius: 20rpx;
  width: 95%;
  height: 86%;
  align-self: center;
  border: solid 1px white;
}

#addCommand {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  margin-bottom: 120rpx;
  margin-right: 60rpx;
  right: 0px;
  bottom: 0px;
}

#bg{
  position: absolute;
  height: 100%;
  width: 100%;
  background: #f5f5f5;
}
